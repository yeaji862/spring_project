<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="AreaDAO">

	<select id="getAreaList" resultType="area">
		SELECT * FROM AREA
	</select>
	
	<select id="getArea" resultType="area">
		SELECT * FROM AREA WHERE AREA_NAME = #{area_name}
	</select>
	
	<select id="getAreaNum" resultType="area">
		SELECT * FROM AREA WHERE AREA_NUM = #{area_num}
	</select>

</mapper>